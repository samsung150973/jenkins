pipeline {
    agent any

    environment {
        ENV_URL = "pipeline.learning.com"
    }
    stages {
        
        stage('stage Name 1') {
            steps {
                sh "echo This is my first stage in jenkins world"
            }
        }   

        stage('stage Name 2') {
            steps {
                sh "echo This is the environment variable $ENV_URL"
            }
        } 
        
    }
}